{
  "name": "fond-of-spryker/conditional-availability-api",
  "description": "Conditional availability api module",
  "license": "MIT",
  "authors": [
    {
      "name": "Daniel Rose",
      "email": "daniel-rose@gmx.de"
    }
  ],
  "require": {
    "php": ">=7.1",
    "spryker/api": "^0.1.0 || ^0.2.0 || ^0.3.0",
    "spryker/api-query-builder": "^0.1.0",
    "fond-of-spryker/conditional-availability": "dev-master"
  },
  "require-dev": {
    "codeception/codeception": "^2.4",
    "fond-of-codeception/spryker": "^1.0.0",
    "php-coveralls/php-coveralls": "^2.1",
    "phpro/grumphp": "^0.14",
    "sebastian/phpcpd": "^4.0",
    "spryker/code-sniffer": "^0.12"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "FondOfSpryker\\": "src/FondOfSpryker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FondOfSpryker\\": "tests/FondOfSpryker/",
      "Generated\\":"src/Generated/"
    }
  }
}
